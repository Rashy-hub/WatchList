@model List<Watchlist.Models.MovieViewModel>




<h1>@ViewData["Title"]</h1>

<table class="table">
    <thead>
        <tr>
            <th>Title</th>
            <th>Year</th>
            <th>Has Seen</th>
            <th>Rating</th>
        </tr>
    </thead>
    <tbody>
        @foreach (var movie in Model)
        {

            <tr>

                <td>@movie.Title</td>
                <td>@movie.Year</td>
                <td>@movie.hasSeen</td>
                <td>@movie.Rating</td>
            </tr>


        }
    </tbody>
</table>
